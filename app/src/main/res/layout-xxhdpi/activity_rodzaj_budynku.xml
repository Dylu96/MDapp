<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:theme="@style/Theme.MDApp.NoActionBar"
    tools:context=".RodzajBudynku">

    <TextView
        android:id="@+id/wynikrodzajbudynku"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:gravity="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/DalejLiczbaMieszkancow"
        tools:layout_editor_absoluteX="0dp" />

    <ImageView
        android:id="@+id/rodzaj7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/DalejLiczbaMieszkancow"
        app:layout_constraintTop_toBottomOf="@+id/rodzaj6"
        app:srcCompat="@android:drawable/ic_dialog_info" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:gravity="right"
        android:text="Energooszczędny"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/rodzaj3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/rodzaj3"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:gravity="right"
        android:text="Dobrze izolowany"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/rodzaj4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/rodzaj4"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="101-150"
        app:layout_constraintBottom_toBottomOf="@+id/rodzaj7"
        app:layout_constraintEnd_toStartOf="@+id/rodzaj7"
        app:layout_constraintStart_toStartOf="@+id/DalejLiczbaMieszkancow"
        app:layout_constraintTop_toTopOf="@+id/rodzaj7"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="61-90"
        app:layout_constraintBottom_toBottomOf="@+id/rodzaj5"
        app:layout_constraintEnd_toEndOf="@+id/textView18"
        app:layout_constraintStart_toStartOf="@+id/textView18"
