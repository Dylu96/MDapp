        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/rodzaj2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toStartOf="@+id/rodzaj3"
        app:layout_constraintTop_toBottomOf="@+id/rodzaj1"
        app:srcCompat="@android:drawable/ic_dialog_info" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:gravity="right"
        android:text="Słaba izolacja"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/rodzaj6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/rodzaj6"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/rodzaj5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toStartOf="@+id/rodzaj6"
        app:layout_constraintTop_toBottomOf="@+id/rodzaj4"
        app:srcCompat="@android:drawable/ic_dialog_info" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="16-40"
        app:layout_constraintBottom_toBottomOf="@+id/rodzaj2"
        app:layout_constraintEnd_toEndOf="@+id/textView15"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView15"
        app:layout_constraintTop_toTopOf="@+id/rodzaj2"
        app:layout_constraintVertical_bias="0.23" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:gravity="right"
        android:text="Nieocieplony"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/rodzaj7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/rodzaj7"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/rodzaj4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toStartOf="@+id/rodzaj5"
        app:layout_constraintTop_toBottomOf="@+id/rodzaj3"
        app:srcCompat="@android:drawable/ic_dialog_info" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:gravity="right"
        android:text="Pasywny"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/rodzaj1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/rodzaj1" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:gravity="right"
        android:text="Niskoenergetyczny"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/rodzaj2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/rodzaj2"
        app:layout_constraintVertical_bias="0.1" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5-15"
        app:layout_constraintBottom_toBottomOf="@+id/rodzaj1"
        app:layout_constraintEnd_toEndOf="@+id/textView14"
        app:layout_constraintStart_toStartOf="@+id/textView14"
        app:layout_constraintTop_toTopOf="@+id/rodzaj1"
        app:layout_constraintVertical_bias="0.538" />

    <ImageView
        android:id="@+id/rodzaj6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toStartOf="@+id/rodzaj7"
        app:layout_constraintTop_toBottomOf="@+id/rodzaj5"
        app:srcCompat="@android:drawable/ic_dialog_info" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="91-100"
        app:layout_constraintBottom_toBottomOf="@+id/rodzaj6"
        app:layout_constraintEnd_toEndOf="@+id/textView19"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/textView19"
        app:layout_constraintTop_toTopOf="@+id/rodzaj6"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="41-50"
        app:layout_constraintBottom_toBottomOf="@+id/rodzaj3"
        app:layout_constraintEnd_toEndOf="@+id/textView16"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView16"
        app:layout_constraintTop_toTopOf="@+id/rodzaj3"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="51-60"
        app:layout_constraintBottom_toBottomOf="@+id/rodzaj4"
        app:layout_constraintEnd_toEndOf="@+id/textView17"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView17"
        app:layout_constraintTop_toTopOf="@+id/rodzaj4"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/rodzaj3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toStartOf="@+id/rodzaj4"
        app:layout_constraintTop_toBottomOf="@+id/rodzaj2"
        app:srcCompat="@android:drawable/ic_dialog_info" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:gravity="right"
        android:text="Standardowa izolacja"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/rodzaj5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/rodzaj5"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/rodzaj1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="@+id/rodzaj2"
        app:layout_constraintTop_toBottomOf="@+id/textView21"
        app:srcCompat="@android:drawable/ic_dialog_info" />

    <TextView
        android:id="@+id/textView37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:text="Rodzaj budynku"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/rodzaj1"
        app:layout_constraintEnd_toStartOf="@+id/textView21"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:inputType="textMultiLine"
        android:text="Współczynnik \n\[kWh/m2/rok]"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/rodzaj1"
        app:layout_constraintHorizontal_bias="0.518"
        app:layout_constraintStart_toStartOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="0dp"
        android:layout_height="120dp"
        android:layout_marginTop="4dp"
        android:src="@drawable/logo_pp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="0dp" />

    <Button
        android:id="@+id/DalejLiczbaMieszkancow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="40dp"
        android:onClick="openTemperaturaBudynku"
        android:text="Dalej"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="16dp"
        android:text="© Copyright by Michał Dylewski 2021"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/wartoscbudynku"
        android:layout_width="162dp"
        android:layout_height="47dp"
        android:ems="10"
        android:gravity="center"
        android:hint="Podaj wartość"
        android:inputType="numberDecimal"
        app:layout_constraintBottom_toTopOf="@+id/wynikrodzajbudynku"
        app:layout_constraintEnd_toStartOf="@+id/zaakceptujrodzaj"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/zaakceptujrodzaj"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="40dp"
        android:text="Zaakceptuj"
        app:layout_constraintBottom_toTopOf="@+id/wynikrodzajbudynku"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>